{
  "name": "@adhawk/react-error-boundary",
  "description": "A React component for catching and reporting errors",
  "version": "1.1.7",
  "author": "developers@tryadhawk.com",
  "license": "MIT",
  "module": "./lib/react-error-boundary.esm.js",
  "unpkg": "./lib/react-error-boundary.umd.min.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "@adhawk/feathers"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -Rf ./lib/* ./meta/bundlesize/* ./meta/coverage/* ./.rpt2_cache",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "postbuild": "rollup -c",
    "watch": "tsc-watch --onSuccess \"npm run postbuild\" -p .",
    "predeploy": "npm run build",
    "deploy": "npm publish",
    "test": "jest --config ../../config/jest.config.js --testPathPattern packages/react-error-boundary",
    "test:watch": "jest --config ../../config/jest.config.js --testPathPattern packages/react-error-boundary --watch",
    "test:cjs": "npm run build && jest --config ../../config/jest.cjs.config.js --testPathPattern packages/react-error-boundary"
  },
  "dependencies": {
    "@adhawk/error-tracking": "^1.1.5",
    "tslib": "^1.10.0"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/react": "^16.9.17",
    "jest": "24.9.0",
    "react": "^16.12.0",
    "tsc-watch": "4.1.0",
    "typescript": "3.7.5"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "gitHead": "21d4253fee28bfb703c7ca25122f230a86669d3f"
}
