{
  "name": "@adhawk/ctm-analytics",
  "description": "Analytics.js integration for CallTrackingMetrics",
  "version": "1.0.1",
  "author": "developers@tryadhawk.com",
  "license": "MIT",
  "main": "./lib/ctm-analytics.cjs.js",
  "module": "./lib/ctm-analytics.esm.js",
  "unpkg": "./lib/ctm-analytics.umd.min.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "@adhawk/feathers"
  },
  "sideEffects": true,
  "scripts": {
    "clean": "rm -Rf ./lib/* ./meta/bundlesize/* ./meta/coverage/* ./.rpt2_cache",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "postbuild": "rollup -c",
    "predeploy": "npm run build",
    "deploy": "npm publish",
    "test": "jest --config ../..//jest.config.js --testPathPattern packages/ctm-analytics",
    "test:watch": "jest --config ../..//jest.config.js --testPathPattern packages/ctm-analytics --watch",
    "test:cjs": "npm run build && jest --config ../..//jest.cjs.config.js --testPathPattern packages/ctm-analytics",
    "test:umd": "npm run build && jest --config ../..//jest.umd.config.js --testPathPattern packages/ctm-analytics"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@segment/analytics.js-integration-tester": "^3.1.1",
    "@segment/clear-env": "^2.1.1",
    "jest-environment-jsdom": "^25.1.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "@segment/analytics.js-core": "3.10.1",
    "@segment/analytics.js-integration": "^3.3.1",
    "analytics.js-integrations": "https://github.com/segmentio/analytics.js-integrations#c1fc75c3913035d9b1d187e3948d8ef257a32fc2",
    "component-type": "^1.2.1"
  }
}
