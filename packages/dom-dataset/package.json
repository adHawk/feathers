{
  "name": "@adhawk/dom-dataset",
  "description": "Easy access to a DOM dataset",
  "version": "1.1.4",
  "author": "developers@tryadhawk.com",
  "license": "MIT",
  "main": "./lib/dom-dataset.cjs.js",
  "module": "./lib/dom-dataset.esm.js",
  "unpkg": "./lib/dom-dataset.umd.min.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "@adhawk/feathers"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -Rf ./lib/* ./meta/bundlesize/* ./meta/coverage/* ./.rpt2_cache",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "postbuild": "rollup -c",
    "predeploy": "npm run build",
    "deploy": "npm publish",
    "test": "jest --config ../../config/jest.config.js --testPathPattern packages/dom-dataset",
    "test:watch": "jest --config ../../config/jest.config.js --testPathPattern packages/dom-dataset --watch",
    "test:cjs": "npm run build && jest --config ../../config/jest.cjs.config.js --testPathPattern packages/dom-dataset",
    "test:umd": "npm run build && jest --config ../../config/jest.umd.config.js --testPathPattern packages/dom-dataset"
  },
  "dependencies": {
    "tslib": "^1.10.0"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "jest": "^25.1.0",
    "rollup": "1.31.1",
    "typescript": "3.8.2"
  }
}
