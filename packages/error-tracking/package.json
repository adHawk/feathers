{
  "name": "@adhawk/error-tracking",
  "description": "Configure error tracking for your project",
  "version": "1.1.4",
  "author": "developers@tryadhawk.com",
  "license": "MIT",
  "main": "./lib/error-tracking.cjs.js",
  "module": "./lib/error-tracking.esm.js",
  "unpkg": "./lib/error-tracking.umd.min.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "@adhawk/feathers"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -Rf ./lib/* ./meta/bundlesize/* ./meta/coverage/* ./.rpt2_cache",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "postbuild": "rollup -c",
    "watch": "tsc-watch --onSuccess \"npm run postbuild\" -p .",
    "predeploy": "npm run build",
    "deploy": "npm publish",
    "test": "jest --config ../../config/jest.config.js --testPathPattern packages/error-tracking",
    "test:watch": "jest --config ../../config/jest.config.js --testPathPattern packages/error-tracking --watch",
    "test:cjs": "npm run build && jest --config ../../config/jest.cjs.config.js --testPathPattern packages/error-tracking",
    "test:umd": "npm run build && jest --config ../../config/jest.umd.config.js --testPathPattern packages/error-tracking"
  },
  "dependencies": {
    "@sentry/browser": "^5.11.0",
    "tslib": "^1.10.0"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.0.0",
    "jest": "24.9.0",
    "rollup": "1.30.0",
    "tsc-watch": "4.0.0",
    "typescript": "3.7.5"
  },
  "gitHead": "21d4253fee28bfb703c7ca25122f230a86669d3f"
}
