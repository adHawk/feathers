{
  "name": "@adhawk/react-tracked-link",
  "description": "A React component for catching and reporting errors",
  "version": "0.0.1",
  "author": "developers@tryadhawk.com",
  "license": "MIT",
  "module": "./lib/react-tracked-link.esm.js",
  "unpkg": "./lib/react-tracked-link.umd.min.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "@adhawk/feathers"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -Rf ./lib/* ./meta/bundlesize/* ./meta/coverage/* ./.rpt2_cache",
    "prepare": "npm run build",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "postbuild": "rollup -c",
    "watch": "tsc-watch --onSuccess \"npm run postbuild\" -p .",
    "predeploy": "npm run build",
    "deploy": "npm publish",
    "test": "jest --config ../../config/jest.config.js --testPathPattern packages/react-tracked-link",
    "test:watch": "jest --config ../../config/jest.config.js --testPathPattern packages/react-tracked-link --watch",
    "test:cjs": "npm run build && jest --config ../../config/jest.cjs.config.js --testPathPattern packages/react-tracked-link"
  },
  "dependencies": {
    "tslib": "^1.10.0"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@types/segment-analytics": "^0.0.32",
    "jest": "24.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "tsc-watch": "4.0.0",
    "typescript": "3.7.5"
  },
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "gitHead": "21d4253fee28bfb703c7ca25122f230a86669d3f"
}
